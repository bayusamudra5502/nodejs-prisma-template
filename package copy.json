{
  "name": "pga-service",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "start": "node ./build/main.js",
    "pretty": "prettier --write './src/*.ts'",
    "prepare": "husky install",
    "precommit": "yarn pretty",
    "clean": "rm -rf build",
    "build": "babel ./src --ignore node_modules --out-dir ./build --minified --extensions '.ts,.js'",
    "test": "jest"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "ajv": "^8.12.0",
    "axios": "^1.2.2",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-auth-library": "^8.7.0",
    "multer": "^1.4.5-lts.1",
    "validator": "^13.7.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/plugin-transform-typescript": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.20.7",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.10",
    "babel-plugin-module-resolver": "^4.1.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prisma": "^4.8.1",
    "superagent": "^8.0.6",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "repository": "git@gitlab.com:putra-putri-pga/pga-service.git",
  "author": "bayusamudra5502 <bayusamudra.55.02.com@gmail.com>",
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@controllers/(.*)$": "<rootDir>/src/controllers/$1",
      "^@models/(.*)$": "<rootDir>/src/models/$1",
      "^@routes/(.*)$": "<rootDir>/src/routes/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@middlewares/(.*)$": "<rootDir>/src/middlewares/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1"
    }
  }
}
